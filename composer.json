{
    "name": "alex-hnydiuk/laracent",
    "type": "library",
    "description": "Centrifugo broadcaster for laravel >= 5.7 and Centrifugo >= 2.1.0",
    "keywords": ["laravel", "centrifugo", "broadcaster"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Hnydiuk",
            "email": "o.hnydiuk@gmail.com"
        }
    ],
    "homepage": "https://github.com/AlexHnydiuk/laracent",
    "support": {
        "issues": "https://github.com/AlexHnydiuk/laracent/issues",
        "source": "https://github.com/AlexHnydiuk/laracent"
    },
    "require": {
        "php": ">=7.2.12",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/broadcasting": "^5.3"
    },
    "require-dev": {
        "orchestra/testbench": "^3.3",
        "phpunit/phpunit": "^5.7|^6.1"
    },
    "autoload": {
        "psr-4": {
            "AlexHnydiuk\\Laracent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexHnydiuk\\Laracent\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexHnydiuk\\Laracent\\LaracentServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}